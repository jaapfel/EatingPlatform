        hsp = -2.5;
        image_xscale = -1;
    }
    if(food = 5)
    {
        hsp = -2;
        image_xscale = -1;
    }
}
if (Key_Right)
{
    if(food = 1)
    {
        hsp = 4;
        image_xscale = 1;
    }
    if(food = 2)
    {
        hsp = 3.5;
        image_xscale = 1;
    }
    if(food = 3)
    {
        hsp = 3;
        image_xscale = 1;
    }
    if(food = 4)
    {
        hsp = 2.5;
        image_xscale = 1;
    }
    if(food = 5)
    {
        hsp = 2;
        image_xscale = 1;
    }
}

if ((Key_Right &amp;&amp; Key_Left) or (!Key_Right &amp;&amp; !Key_Left))
{
    hsp = 0;
}

if (Key_Jump)
{
    if(grounded)
    {
        vsp = -9;
        dj = 1;
    }
//jumps forever if dj, only jumps once if dj=1
 /*   if(dj)
    {
        vsp = -9;
        dj = 1;
    }*/
}

vsp += grav;

//Vertical Colllision
if place_meeting(x,y+vsp,par_box)
{
    while(!place_meeting(x,y+1,par_box)) y+=1;
    vsp = 0;
    grounded = 1;
}
else 
{
    grounded = 0;
}

//Horizontal Collision
if place_meeting(x+hsp,y,par_box)
{
    while(!place_meeting(x+sign(hsp),y,par_box)) x+=sign(hsp);
    hsp =0;
}

x += hsp;
y += vsp;

//Weight Classes
if(food = 1)
{
    sprite_index = ball_1;
    grav = 0.4;
}
if(food = 2)
