x = min(x, room_width-20);
x = max(x, 20);
y = min(y, room_height-20);
y = max(y, 0);

if (Key_Left)
{
    if(food = 1)
    {
        hsp = -4;
        image_xscale = -1;
    }
    if(food = 2)
    {
        hsp = -3.5;
        image_xscale = -1;
    }
    if(food = 3)
    {
        hsp = -3;
        image_xscale = -1;
    }
    if(food = 4)
